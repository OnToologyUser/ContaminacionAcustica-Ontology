        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
        <rdfs:label xml:lang="es">está en esquema</rdfs:label>
        <rdfs:label xml:lang="en">is in scheme</rdfs:label>
    </owl:ObjectProperty>
    


